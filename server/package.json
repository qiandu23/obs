{
  "name": "object-storage-browser",
  "version": "1.0.0",
  "description": "The browser for object storage",
  "scripts": {
    "check": "eslint server/bin/** server/src/** server/test/**",
    "test": "nyc mocha --recursive",
    "api-server": "node bin/command.js start-api",
    "build-linux": "pkg .. --targets node14-linux-x64 --output obs-browser",
    "build-macos": "pkg .. --targets node14-macos-x64 --output obs-browser",
    "build-win": "pkg .. --targets node14-win-x64 --output obs-browser.exe"
  },
  "author": "org-qiandu",
  "license": "MIT",
  "bin": "bin/command.js",
  "pkg": {
    "targets": [
      "node16-linux-x64"
    ],
    "output": [
      "obs-ui"
    ],
    "assets": [
    ]
  },
  "dependencies": {
    "async": "^3.2.0",
    "aws-sdk": "^2.1004.0",
    "axios": "^0.21.1",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "commander": "^8.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "iconv-lite": "^0.6.3",
    "jose": "^4.14.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "pino": "^6.11.3",
    "raw-body": "^2.5.1",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0"
  }
}
